{
  "name": "@observablehq/cli",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/observablehq/cli.git"
  },
  "files": [
    "src/**/*.js"
  ],
  "scripts": {
    "dev": "tsx watch ./src/preview.ts",
    "test": "yarn test:mocha && yarn test:lint && yarn test:prettier",
    "test:mocha": "mkdir -p test/output && node --loader tsx ./node_modules/.bin/mocha 'test/**/*-test.*'",
    "test:lint": "eslint src test",
    "test:prettier": "prettier --check src test"
  },
  "dependencies": {
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "highlight.js": "^11.8.0",
    "markdown-it": "^13.0.2",
    "send": "^0.18.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.2",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.7.1",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
